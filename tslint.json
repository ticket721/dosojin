{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "no-console": [
            false
        ],
        "typedef": [
            true,
            "call-signature",
            "arrow-call-signature",
            "parameter",
            "arrow-parameter",
            "property-declaration",
            "member-variable-declaration",
            "object-destructuring",
            "array-destructuring"
        ]
    },
    "rulesDirectory": []
}